{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/index.js",
    "build": "tsc -p tsconfig.prod.json",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest unit",
    "test:integration": "dotenv -e ./.env.test yarn generate:reset && dotenv -e ./.env.test jest int -i",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint --fix .",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:migration": "npx prisma migrate dev",
    "generate:reset": "npx prisma migrate reset --force",
    "prisma:studio": "npx prisma studio",
    "test:environment:docker": "yarn docker:up && yarn docker:test:run & yarn docker:down",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker compose down",
    "docker:test:run": "docker exec server ./bin/wait-for-it.sh db:5432 -- yarn test",
    "compile": "npm run generate && tsc"
  },
  "keywords": [],
  "author": "Bobby",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-codegen/core": "^2.6.5",
    "@graphql-codegen/plugin-helpers": "^2.7.2",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@prisma/client": "^4.5.0",
    "apollo-graphql": "^0.9.7",
    "apollo-server": "^3.11.1",
    "dotenv-cli": "^6.0.0",
    "dotenv-safe": "^8.2.0",
    "graphql": "^16.6.0"
  }
}
